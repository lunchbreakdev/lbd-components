{
  "name": "@lunchbreakdev/lbd-components",
  "version": "0.0.0",
  "description": "A web component library for creating components with accessible markup",
  "main": "./dist/lbd-components.min.js",
  "types": "./dist/lbd-components.d.ts",
  "type": "module",
  "scripts": {
    "start": "yarn build --watch",
    "build": "tsc && npx esbuild lbd-components.ts --bundle --minify --format=esm --outfile=dist/lbd-components.min.js"
  },
  "dependencies": {
    "@lunchbreakdev/lbd-breadcrumb": "0.0.0",
    "@lunchbreakdev/lbd-disclosure": "0.0.0",
    "@lunchbreakdev/lbd-tabs": "0.0.0",
    "@lunchbreakdev/lbd-tooltip": "0.0.0",
    "@lunchbreakdev/lbd-treeview": "0.0.0"
  },
  "devDependencies": {
    "typescript": "^5.4.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lunchbreakdev/lbd-components.git",
    "directory": "./packages/components"
  },
  "keywords": [
    "web",
    "component",
    "components",
    "web components",
    "custom elements",
    "component library"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "bugs": {
    "url": "https://github.com/lunchbreakdev/lbd-components/issues"
  },
  "author": {
    "name": "Morgan Spencer",
    "email": "hello@lunchbreak.dev",
    "url": "https://lunchbreak.dev"
  },
  "homepage": "https://github.com/lunchbreakdev/lbd-components/tree/main/packages/components#readme"
}
